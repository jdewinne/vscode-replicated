{
  "name": "replicated",
  "displayName": "Replicated",
  "description": "Lint your kubernetes manifest files using Replicated linting service",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:replicated.lint.enable"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "replicated.lint.enable",
        "title": "Replicated Lint Enable"
      },
      {
        "command": "replicated.lint.disable",
        "title": "Replicated Lint Disable"
      }
    ],
    "configuration": {
      "title": "Replicated",
      "properties": {
        "replicated.manifestsFolder": {
          "type": "string",
          "scope": "resource",
          "default": "manifests",
          "description": "%config.manifestsFolder%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "got": "11.8.3",
    "tar": "^6.1.13"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/tar": "^6.1.3",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  }
}
